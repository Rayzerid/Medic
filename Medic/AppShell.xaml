<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Medic.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Medic"
    xmlns:view="clr-namespace:Medic.View"
    Shell.FlyoutBehavior="Disabled">

    <ShellContent
        ContentTemplate="{DataTemplate view:NewPage1}"
        Route="MainPage"
        Shell.TabBarIsVisible="False" />
    <ShellContent
        ContentTemplate="{DataTemplate view:DashboardView}"
        Route="DashboardView"
        Shell.TabBarIsVisible="False" />
    <ShellContent
        ContentTemplate="{DataTemplate view:EmailCheckView}"
        Route="EmailCheckView"
        Shell.TabBarIsVisible="False" />
    <ShellContent
        ContentTemplate="{DataTemplate view:SecurityView}"
        Route="SecurityView"
        Shell.TabBarIsVisible="False" />
    <ShellContent
        ContentTemplate="{DataTemplate view:RgistrationView}"
        Route="RgistrationView"
        Shell.TabBarIsVisible="False" />
    <ShellContent
        ContentTemplate="{DataTemplate view:AnalyzesView}"
        Route="AnalyzesView"
        Shell.TabBarIsVisible="False" />

</Shell>
